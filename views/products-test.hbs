<div class="container">
  <h1> {{title}} </h1>
  {{#if products}}
  <table class="table">
    <thead>
      <tr>
        <th scope="col">#ID</th>
        <th scope="col">Title</th>
        <th scope="col">Price</th>
        <th scope="col">Image</th>
      </tr>
    </thead>
    <tbody>
      {{#each products}}
        <tr>
          <th scope="row"> {{id}} </th>
          <td> {{title}} </td>
          <td> {{price}} </td>
          <td><img src={{thumbnail}}  alt="" border="3" height="100" width="100" /></td>
        </tr>
      {{/each}}
    </tbody>
  </table>
  {{else}}
    <div align="center">NO FOUND PRODUCT</div>
  {{/if}}
</div>