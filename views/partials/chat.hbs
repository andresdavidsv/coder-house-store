<div class="container">
  <h1 class="mt-5"> {{titleChat}} </h1>
  <h1 class="mt-5" id="numberCompresion"></h1>
  <hr>
  <form id="chatForm">
    <div class="row">
      <div class="col-4">
        <input type="email" class="form-control" id="userMail" placeholder="user@mail.com" >
      </div>
      <div class="col-4">
        <input type="text" class="form-control" id="userName" placeholder="First Name" >
      </div>
      <div class="col-4">
        <input type="text" class="form-control" id="userLastName" placeholder="Last Name" >
      </div>
      <div class="col-4">
        <input type="number" class="form-control" id="userAge" placeholder="Age" >
      </div>
      <div class="col-4">
        <input type="text" class="form-control" id="userAlias" placeholder="Alias" >
      </div>
      <div class="col-4">
        <input type="url" class="form-control" id="userAvatar" placeholder="Avatar" >
      </div>
      <div class="col-8">
        <input type="text" class="form-control" id="chatTxtMsg" placeholder="Message" >
      </div>
      <div class="col-8" type="submit">
        <button class="btn btn-primary">Send</button>
      </div>
    </div>
  </form>
  <div class="row">
    <div class="col">
      <ul class="mt-2" id="chatMsgs"></ul>
    </div>
  </div>
</div>