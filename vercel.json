{
  "name": "coder-house-store",
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "env": {
    "DB_USER": "",
    "DB_PASSWORD": "",
    "DB_HOST": "",
    "DB_NAME": "",
    "AUTH_JWT_SECRET": ""
  }
}